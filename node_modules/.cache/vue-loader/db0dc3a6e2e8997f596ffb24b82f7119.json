enu>\r\n\t\t</el-menu>\r\n\t\t<!-- 右上角按钮组 -->\r\n\t\t<el-row class=\"right-btns\">\r\n\t\t\t<el-tooltip v-for=\"(item, idx) in buttonItems\" :key=\"idx\" effect=\"light\" :content=\"item.name\" placement=\"bottom\">\r\n\t\t\t\t<el-button :type=\"item.type\" size=\"small\" :icon=\"item.icon\" @click=\"execute(item.code)\" circle></el-button>\r\n\t\t\t</el-tooltip>\r\n\t\t</el-row>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\t\r\n\timport chartsItems from './charts-items';\r\n\t\r\n\texport default {\r\n\t\tcomponents: {\r\n\t\t\tchartsItems\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tbuttonItems: [\n\t\t\t\t\t{\n\t\t\t\t\t\tname: '返回',\n\t\t\t\t\t\tcode: 'back',\n\t\t\t\t\t\ttype: 'info',\n\t\t\t\t\t\ticon: 'el-icon-back'\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: '保存',\r\n\t\t\t\t\t\tcode: 'save',\r\n\t\t\t\t\t\ttype: 'success',\r\n\t\t\t\t\t\ticon: 'el-icon-folder-checked'\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: '预览',\r\n\t\t\t\t\t\tcode: 'preview',\r\n\t\t\t\t\t\ttype: 'warning',\r\n\t\t\t\t\t\ticon: 'el-icon-view'\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: '发布',\r\n\t\t\t\t\t\tcode: 'emit',\r\n\t\t\t\t\t\ttype: 'primary',\r\n\t\t\t\t\t\ticon: 'el-icon-s-promotion'\r\n\t\t\t\t\t},\r\n\t\t\t\t\t{\r\n\t\t\t\t\t\tname: '删除',\r\n\t\t\t\t\t\tcode: 'delete',\r\n\t\t\t\t\t\ttype: 'danger',\r\n\t\t\t\t\t\ticon: 'el-icon-delete'\r\n\t\t\t\t\t}\r\n\t\t\t\t]\r\n\t\t\t}\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\texecute(code) {\r\n\t\t\t\t// 发送操作指令\n\t\t\t\tif (code === 'back') {\n\t\t\t\t\tthis.$router.push('/');\n\t\t\t\t}\r\n\t\t\t}\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\t.right-btns {\r\n\t\tposition: absolute;\r\n\t\tright: 10px;\r\n\t\ttop: 0;\r\n\t}\r\n\t.el-button--mini, \r\n\t.el-button--small {\r\n\t\tfont-size: 10px;\r\n\t}\n\t.el-menu.el-menu--horizontal {\n\t\tborder-bottom: none;\n\t}\r\n</style>\r\n"]}]}