roup.chk === o.code }\" @click=\"tabsBtnGroupClick(o.code)\">\r\n\t\t\t\t<el-tooltip class=\"item\" :content=\"o.name\" placement=\"bottom\">\r\n\t\t\t\t\t<i :class=\"o.icon\"></i>\r\n\t\t\t\t</el-tooltip>\r\n\t\t\t</li>\r\n\t\t</ul>\n\t\t<!-- 当前元素名称 -->\n\t\t<el-divider content-position=\"left\">{{ element.eleName }}</el-divider>\n\t\t<!-- 1. 属性面板 -->\n\t\t<div v-show=\"tabsVisible(0)\" class=\"prop-setting\">\n\t\t\t<!-- 基础属性（针对所有组件） -->\n\t\t\t<ec-basics />\n\t\t\t<!-- echarts 组件的配置面板 -->\n\t\t\t<div v-show=\"elemCpntType === 'echarts'\" class=\"components-panel\">\n\t\t\t\t\n\t\t\t</div>\n\t\t</div>\n\t\t<!-- 2. 数据面板 -->\n\t\t<div v-show=\"tabsVisible(1)\" class=\"prop-setting\">\n\t\t\t\n\t\t</div>\n\t\t<!-- 3. 交互面板 -->\n\t\t<div v-show=\"tabsVisible(2)\" class=\"prop-setting\">\n\t\t\t\n\t\t</div>\r\n\t\t\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\t// 组件导入\r\n\timport Basics from './config-items/i-basics';\r\n\t/* import DataZoom from './config-items/DataZoom';\r\n\timport Grid from './config-items/Grid';\r\n\timport Legend from './config-items/Legend';\r\n\timport Title from './config-items/Title';\r\n\timport Tooltip from './config-items/Tooltip';\r\n\timport Toolbox from './config-items/Toolbox';\r\n\timport Xaxis from './config-items/Xaxis';\r\n\timport Yaxis from './config-items/Yaxis';\r\n\timport Series from './config-items/Series';\r\n\timport DataSetting from './config-items/DataSetting';\r\n\timport GlText from './config-items/GlText'; */\r\n\t\r\n\texport default {\r\n\t\tcomponents: {\r\n\t\t\t'ec-basics': Basics,\r\n\t\t\t/* 'ec-datazoom': DataZoom,\r\n\t\t\t'ec-grid': Grid,\r\n\t\t\t'ec-legend': Legend,\r\n\t\t\t'ec-title': Title,\r\n\t\t\t'ec-toolbox': Toolbox,\r\n\t\t\t'ec-tooltip': Tooltip,\r\n\t\t\t'ec-xaxis': Xaxis,\r\n\t\t\t'ec-yaxis': Yaxis,\r\n\t\t\t'ec-series': Series,\r\n\t\t\tDataSetting,\r\n\t\t\t'gl-text': GlText */\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\t// tabs 标签按钮组\r\n\t\t\t\ttabsBtnGroup: {\r\n\t\t\t\t\tlis: [\r\n\t\t\t\t\t\t{\r\n\t\t\t\t\t\t\tcode: 'options',\r\n\t\t\t\t\t\t\tname: '属性',\r\n\t\t\t\t\t\t\ticon: 'el-icon-s-operation'\r\n\t\t\t\t\t\t},\r\n\t\t\t\t\t\t{\r\n\t\t\t\t\t\t\tcode: 'data',\r\n\t\t\t\t\t\t\tname: '数据',\r\n\t\t\t\t\t\t\ticon: 'el-icon-sort'\r\n\t\t\t\t\t\t},\r\n\t\t\t\t\t\t{\r\n\t\t\t\t\t\t\tcode: 'interaction',\r\n\t\t\t\t\t\t\tname: '交互',\r\n\t\t\t\t\t\t\ticon: 'el-icon-thumb'\r\n\t\t\t\t\t\t}\r\n\t\t\t\t\t],\r\n\t\t\t\t\tchk: 'options'\r\n\t\t\t\t},\r\n\t\t\t\t// 当前激活元素的 echarts 的配置项对象\r\n\t\t\t\telemEcOpts: {},\r\n\t\t\t\t// 当前元素名称\r\n\t\t\t\telemName: '',\r\n\t\t\t\t// 当前激活元素的类型（是常规组件还是echarts组件）\r\n\t\t\t\telemCpntType: ''\r\n\t\t\t};\r\n\t\t},\n\t\tcomputed: {\n\t\t\telement() {\n\t\t\t\treturn this.$store.getters._element || {};\n\t\t\t},\n\t\t},\r\n\t\tmounted() {\n\t\t\t\n\t\t},\r\n\t\tmethods: {\n\t\t\t// tabs 标签按钮组显示状态\n\t\t\ttabsVisible(k) {\n\t\t\t\tconst t = ['options', 'data', 'interaction'];\n\t\t\t\treturn this.tabsBtnGroup.chk === t[k];\n\t\t\t},\r\n\t\t\t// tabs 标签按钮组点击事件\r\n\t\t\ttabsBtnGroupClick(code) {\r\n\t\t\t\tthis.tabsBtnGroup.chk = code;\r\n\t\t\t},\r\n\t\t\t// 是否显示指定配置项\r\n\t\t\tshowConfigItem(t) {\r\n\t\t\t\treturn !!this.elemEcOpts[t];\r\n\t\t\t}\r\n\t\t},\r\n\t}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n\t@import '../../../assets/layout/i-element-config.less';\r\n</style>\r\n"]}]}